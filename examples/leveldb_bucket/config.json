{
  "local": {
    "mapBucket": {
      "bucketType": "filesystem",
      "config": {
        "directory": "/"
      }
    },
    "interBucket": {
      "bucketType": "filesystem",
      "config": {
        "directory": "/tmp/kmr-yc/${JOBNAME}-inter"
      }
    },
    "flushBucket": {
      "bucketType": "filesystem",
      "config": {
        "directory": "/tmp/kmr-yc/${JOBNAME}-inter"
      }
    },
    "reduceBucket": {
      "bucketType": "filesystem",
      "config": {
        "directory": "/tmp/kmr-yc/${JOBNAME}"
      }
    }
  },
  "remote": {
    "podDescription": {
      "volumeMounts": [
        {
          "name": "cephfs",
          "mountPath": "/cephfs"
        }
      ],
      "volumes": [
        {
          "name": "cephfs",
          "hostPath": {
            "path": "/mnt/cephfs"
          }
        }
      ]
    },
    "mapBucket": {
      "bucketType": "filesystem",
      "config": {
        "directory": "/cephfs"
      }
    },
    "interBucket": {
      "bucketType": "filesystem",
      "config": {
        "directory": "/cephfs/kmr/${JOBNAME}-inter"
      }
    },
    "flushBucket": {
      "bucketType": "filesystem",
      "config": {
        "directory": "/tmp/kmr/${JOBNAME}-inter"
      }
    },
    "reduceBucket": {
      "bucketType": "filesystem",
      "config": {
        "directory": "/cephfs/kmr/${JOBNAME}"
      }
    },
    "dockerRegistry": "in.fds.so:5000",
    "namespace": "kmr",
    "serviceAccount": "kmr-master"
  }
}
